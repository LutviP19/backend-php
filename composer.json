{
    "name": "lutvip19/backend-php",
    "license": "MIT",
    "description": "Simple PHP framework for backend services.",
    "keywords": ["mvc", "event-driven", "asynchronous", "microservice", "framework"],
    "require": {
        "php": "^8.2",
        "symfony/console": "^7.3",
        "symfony/event-dispatcher": "^7.3",
        "symfony/messenger": "^7.3",
        "symfony/mailer": "^7.3",
        "php-amqplib/php-amqplib": "^3.7",
        "amphp/amp": "^3.1",
        "revolt/event-loop": "^1.0",
        "monolog/monolog": "^3.9",
        "vlucas/phpdotenv": "^5.6",
        "roolith/event": "^1.0",
        "robinvdvleuten/ulid": "^5.0",
        "rbdwllr/reallysimplejwt": "^5.0",
        "maba/gentle-force": "^0.3.1",
        "predis/predis": "^1.1",
        "psr/http-message": "^2.0",
        "psr/http-server-middleware": "^1.0",
        "openswoole/core": "^22.2",
        "nikic/fast-route": "^1.3",
        "upscale/swoole-session": "^2.1",
        "bayfrontmedia/php-sanitize": "^2.1"
    },
    "bin": [
        "bin/console"
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helpers.php",
            "app/Helpers/inject_php.php"
        ]
    }
}
